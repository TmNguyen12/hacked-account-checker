{
  "name": "hacked-account-checker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "dompurify": "^1.0.4",
    "flow-bin": "^0.72.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "flow": "flow",
    "start": "npm run-script sass:build && react-scripts start",
    "build": "npm run-script sass:build && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow start": "flow start",
    "flow stop": "flow stop",
    "flow status": "flow status",
    "flow coverage": "flow coverage",
    "sass": "sass --watch src/styles/scss:src/styles/css",
    "sass:watch": "node-sass-chokidar -w src/styles/scss -o src/styles/css",
    "sass:build": "node-sass-chokidar src/styles/scss -o src/styles/css --source-map true"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-preset-flow": "^6.23.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.47.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "hoek": "^5.0.3",
    "node-sass-chokidar": "^1.3.0"
  }
}
